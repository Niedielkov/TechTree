<a asp-action="Details" asp-route-id="@Model" class="text-dark mr-4"><i class="fas fa-table"></i></a> 
<a asp-action="Delete" asp-route-id="@Model" class="text-danger mr-4"><i class="fas fa-trash"></i></a> 